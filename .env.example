# Copy to .env and fill your keys
GROQ_API_KEY=<your-groq-api-key>
GROQ_MODEL=llama-3.3-70b-versatile

# Together AI (embeddings)
TOGETHER_API_KEY=<your-together-api-key>
# Optional: override default embeddings model used in src/rag.py
TOGETHER_EMBEDDINGS_MODEL=intfloat/multilingual-e5-large-instruct

# Proxy for YouTube APIs / yt-dlp (optional)
HTTP_URL=http://user:pass@my-custom-proxy.org:port
